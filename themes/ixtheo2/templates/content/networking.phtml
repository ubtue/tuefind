        let markers = {
            'default': "{$this->imageLink('ol/marker.svg')}",
            'comm': "{$this->imageLink('ol/marker.svg')}",
            'bibliographien': "{$this->imageLink('ol/marker-tuebingen.png')}",
            'bibliotheken': "{$this->imageLink('ol/marker-green.png')}",
            'ver': "{$this->imageLink('ol/marker-purple.png')}",
            'fachin': "{$this->imageLink('ol/marker-black.png')}"
        };

        partnersArray.forEach(function(oneParter){
            if(start == oneParter.group) {
                let customIcon = {
                    iconUrl: markers[oneParter.group],
                    iconSize:[20,32.5],
                }
                let myIcon = L.icon(customIcon);

                let iconOptions = {
                    title: oneParter.name,
                    draggable:false,
                    icon:myIcon
                }

                let marker = new L.Marker([oneParter.lat, oneParter.lon] , iconOptions);
                marker.addTo(cmap);
                marker.bindPopup(oneParter.popup).openPopup();
            }

        });

        // Target's GPS coordinates.
        var target = L.latLng(partnersArray[0].lat, partnersArray[0].lon);

        // Set map's center to target with zoom 5.
        cmap.setView(target, 5);
    }

    $('.high-light-marker-group').click(function(){

        $(this).parents('.map-navigation').find('button').each(function(){
            $(this).removeClass( "btn-primary" );
        })
        $(this).find('button').addClass("btn-primary");
        $(this).find('button').blur();

        cmap.remove();
        let lightGroup = $(this).data('group');
        DrawMap(lightGroup);
    })

    DrawMap('comm');
});
EOD;
